{
  "$id": "https://henrygeorgist.github.io/cc-schemas/compute-manifest.schema.json",
    "properties": {
      "manifest_name": {
        "type": "string",
        "title": "Manifest Name",
        "description": "The name for the compute manifest"
      },
      "plugin_definition": {
        "type": "string",
        "title": "Plugin Definition"
      },
      "command": {
        "items": {
          "type": "string"
        },
        "type": "array",
        "title": "Command Override",
        "description": "An optional command override for the plugin"
      },
      "stores": {
        "title": "Stores",
        "type": "array",
        "items":{
          "allOf": [{"$ref": "https://henrygeorgist.github.io/cc-schemas/data-store.schema.json"}]
        }
      },
      "inputs": {
        "title": "Inputs",
        "type": "object",
        "allOf": [{"$ref": "https://henrygeorgist.github.io/cc-schemas/plugin-inputs.schema.json"}]
      },
      "outputs": {
        "title": "Outputs",
        "type": "array",
        "items":{
          "allOf": [{"$ref": "https://henrygeorgist.github.io/cc-schemas/data-source.schema.json"}]
        }
      },
      "actions": {
        "items": {
          "allOf": [{"$ref": "https://henrygeorgist.github.io/cc-schemas/action.schema.json"}]
        },
        "type": "array",
        "title": "Actions"
      },
      "tags": {
        "additionalProperties": {
          "type": "string"
        },
        "type": "object",
        "title": "Tags"
      },
      "retry_attempts": {
        "type": "integer",
        "title": "Retry Attempts Override"
      },
      "job_timeout": {
        "type": "integer",
        "title": "Job Timeout Override"
      },
      "resource_requirements": {
        "items": {
          "allOf": [{"$ref": "https://henrygeorgist.github.io/cc-schemas/resource-requirement.schema.json"}]
        },
        "type": "array",
        "title": "Resource Requirement Overrides"
      }
    },
    "additionalProperties": false,
    "type": "object"
  }